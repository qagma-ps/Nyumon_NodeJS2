<h2>Users Table</h2>
 <table class="table">
     <tr>
       <th>Name</th>
       <th>Email</th>
       <th>Zip Code</th>
     </tr>
     <% users.forEach(user => { %>
     <tr>
       <td><a href="<%= `/users/${user._id}` %>"><%= user.fullName %></a></td>
       <td><%= user.email %></td>
       <td><%= user.zipCode %></td>
     </tr>
    <% }); %>
 </table>
